; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="cybersemiarida"
run/main_scene="res://scenes/main_title.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

CheckpointManager="*res://singleton/CheckpointManager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
DialogManager="*res://prefabs/dialog_manager.tscn"
GameManager="*res://scripts/GameManager.gd"
MusicPlayer="*res://scripts/music_player.gd"

[dialogue_manager]

general/balloon_path="res://dialogues/balloon.tscn"

[display]

window/size/viewport_width=272
window/size/viewport_height=153
window/size/window_width_override=1904
window/size/window_height_override=1071
window/stretch/mode="canvas_items"
window/stretch/scale=0.7
window/name="beta-cybersemiarida"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[global_group]

player=""
checkpoints=""

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations_pot_files=PackedStringArray("res://dialogues/dialogue.dialogue", "res://dialogos/dialogue.dialogue")

